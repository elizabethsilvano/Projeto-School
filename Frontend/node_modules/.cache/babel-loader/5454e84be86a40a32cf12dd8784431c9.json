A,IAJA,CAIA,UAAA,QAAA;AAAA,eAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AAAA,OAJA;AAKA,KAPA;AASA,IAAA,aATA,2BASA;AACA,UAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA;AACA,aAAA,WAAA;AACA;AACA,KAbA;AAeA,IAAA,WAfA,yBAeA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,WAAA,EADA;;AAAA;AAAA;AAAA,uBAGA,KAAA,CACA,wCAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA,MADA,EAEA;AACA,kBAAA,MAAA,EAAA,KADA;AAEA,kBAAA,OAAA,EAAA;AACA,oCAAA;AADA,mBAFA;AAKA,kBAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,OAAA;AALA,iBAFA,CAHA;;AAAA;AAcA,gBAAA,MAAA,CAAA,WAAA;;AAEA,gBAAA,MAAA,CAAA,OAAA,GAAA,IAAA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA;AAhCA;AA3BA,CAAA","sourcesContent":["<template>\n  <div class=\"add-students\">\n    <div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-md-12\">\n          <div class=\"card\">\n            <div class=\"card-header\">\n              <h2>Editar Aluno</h2>\n            </div>\n            <div class=\"card-body\">\n              <div class=\"col-md-6\">\n                <div class=\"alert alert-success\" v-if=\"success\">\n                  Dados salvo com sucesso!\n                </div>\n                <form @submit.prevent=\"submitStudent()\">\n                  <div class=\"form-group\">\n                    <label class=\"label\">Nome</label>\n                    <input\n                      type=\"text\"\n                      class=\"form-control\"\n                      v-model=\"student.name\"\n                      required=\"required\"\n                    />\n                  </div>\n                  <div class=\"form-group\">\n                    <label class=\"label\">Curso</label>\n                    <select\n                      class=\"form-control\"\n                      v-model=\"student.course\"\n                      required=\"required\"\n                    >\n                      <option value=\"Django\">Django</option>\n                      <option value=\"Python\">Python</option>\n                      <option value=\"Ruby\">Ruby</option>\n                      <option value=\"Vue.js\">Vue.js</option>\n                    </select>\n                  </div>\n                  <div class=\"form-group\">\n                    <label class=\"label\">Número da Matrícula</label>\n                    <input\n                      type=\"number\"\n                      class=\"form-control\"\n                      v-model=\"student.number\"\n                      required=\"required\"\n                    />\n                  </div>\n                  <div class=\"form-group\">\n                    <label class=\"label\">Status</label>\n                    <input\n                      type=\"checkbox\"\n                      class=\"form-control\"\n                      v-model=\"student.status\"\n                    />\n                  </div>\n                  <router-link to=\"/students\" class=\"btn btn-info\"\n                    >Voltar</router-link\n                  >\n                  <button class=\"btn btn-success\" :disabled=\"loading\">\n                    <span v-if=\"loading\">\n                      <Circle8></Circle8>\n                    </span>\n                    <span v-if=\"!loading\">Salvar</span>\n                  </button>\n                </form>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { Circle8 } from \"vue-loading-spinner\";\n\nexport default {\n  name: \"Add-Students\",\n  data() {\n    return {\n      student: {\n        id: this.$route.params.id,\n        name: \"\",\n        course: \"\",\n        number: \"\",\n        status: \"\",\n      },\n      success: \"\",\n    };\n  },\n\n  components: {\n    Circle8,\n  },\n\n  props: {\n    loading: { type: Boolean },\n  },\n\n  mounted() {\n    this.showStudent();\n  },\n\n  methods: {\n    showStudent() {\n      axios({\n        method: \"get\",\n        url:\n          \"http://127.0.0.1:8000/api/students/\" + this.$route.params.id + \"/\",\n      }).then((response) => (this.student = response.data));\n    },\n\n    submitStudent() {\n      if (this.$route.params.id) {\n        this.editStudent();\n      }\n    },\n\n    async editStudent() {\n      await this.showStudent();\n\n      await fetch(\n        `http://127.0.0.1:8000/api/students/` + this.$route.params.id + `/`,\n        {\n          method: \"put\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n          },\n          body: JSON.stringify(this.student),\n        }\n      );\n\n      this.showStudent();\n\n      this.success = true;\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}